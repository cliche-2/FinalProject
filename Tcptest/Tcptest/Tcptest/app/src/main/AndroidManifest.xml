<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lke.tcptest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/washing"
        android:label="세탁물 가져가지마조"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".tcpService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".infoActivity" />
        <activity android:name=".finishActivity" />
        <activity android:name=".nfc2Activity" />
        <activity android:name=".info2Activity" />
        <activity android:name=".nfcActivity"></activity>
    </application>

</manifest>